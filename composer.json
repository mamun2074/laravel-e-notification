{
    "name": "mamun2074/laravel-e-notifications",
    "description": "A comprehensive event-based multi-channel notification system for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mamun2074\\ENotification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mamun2074\\ENotification\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mamun2074\\ENotification\\Providers\\ENotificationServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "MD Al Mahmud",
            "email": "mamun120520@email.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "minimum-stability": "dev"
}